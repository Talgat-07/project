{
  "name": "moaproject",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.js": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "*.jsx": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.7",
    "vite": "^5.3.1"
  }
}
